<?xml version='1.0' encoding='utf-8'?>
<widget id="com.iqdev.push" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>PushApp</name>
    <description>
        Push App for testing
    </description>
    <author email="info@iqdevpro.com" href="http://iqdevpro.com">
        IQ' Software Development
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-fcm-config-iqsoftdev" spec="cordova-plugin-fcm-config-iqsoftdev" />
    <plugin name="cordova-plugin-fcm" spec="~1.1.5" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
